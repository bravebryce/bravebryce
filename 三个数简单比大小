#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main(){
	int a = 0;
	int b = 0;
	int c = 0;
	printf("input three number:");
	scanf("%d%d%d", &a, &b, &c);
	int tem = 0;
	if (a < b){
		tem = a;
		a = b;
		b = tem;
	}
	if (a < c){
		tem = a;
		a = c;
		c= tem;
	}
	if (b < c){
		tem = b;
		b =c ;
		c = tem;
	}
	printf("%d\n%d\n%d\n", a, b, c);
	return 0;

}
